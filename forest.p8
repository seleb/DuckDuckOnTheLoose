pico-8 cartridge // http://www.pico-8.com
version 8
__lua__


--math
function lerp(_from,_to,_t)
 return _from+_t*(_to-_from)
end

-- cubic in-out
function ease(t)
 if t >= 0.5 then
  return (t-1)*(2*t-2)*(2*t-2)+1
 else
  return 4*t*t*t
 end
end

function fract(v)
 return v-flr(abs(v))*sgn(v)
end

function v_add(_a,_b)
 return {_a[1]+_b[1],_a[2]+_b[2]}
end
function v_sub(_a,_b)
 return {_a[1]-_b[1],_a[2]-_b[2]}
end
function v_mul(_v,_s)
 return {_v[1]*_s,_v[2]*_s}
end
function v_div(_v,_s)
 if _s != 0 then
  return {_v[1]/_s,_v[2]/_s}
 else
  return {0,0}
 end
end
function v_len(_v)
 return sqrt(_v[1]*_v[1]+_v[2]*_v[2])
end
function v_normalize(_v)
 return v_div(_v,v_len(_v))
end
function v_lerp(_a,_b,_t)
 return{lerp(_a[1],_b[1],_t),lerp(_a[2],_b[2],_t)}
end
function v_dist(_a,_b)
 return v_len(v_sub(_a,_b))
end

function _init()
 --player
 p={}
 p.p={0,0}
 p.v={0,0}
 p.speed={1,1}
 p.a=0
 
 
 -- camera
 cam={}
 cam.p=v_sub(p.p,{64,64})
 
 
 trees={}
 trees.a={}
 trees.gap=32
 for x=0,128,trees.gap do
 for y=0,128,trees.gap do
  local t={}
  t.p={
   x+rnd(trees.gap),
   y+rnd(trees.gap)
  }
  t.leaves={}
  t.height=rnd(10)+5
  t.girth=rnd(5)+5
  add(trees.a,t)
 end
 end
 
 footprints={{p.p[1],p.p[2]}}
end

function _update()
 local v_dif={0,0}
 if btn(0) then
  v_dif[1] -= p.speed[1]
 end
 if btn(1) then
  v_dif[1] += p.speed[1]
 end
 if btn(2) then
  v_dif[2] -= p.speed[1]
 end
 if btn(3) then
  v_dif[2] += p.speed[1]
 end
 
 
 if abs(v_dif[1])+abs(v_dif[2]) > 0.01 then
  p.a=atan2(v_dif[1],v_dif[2])
  p.v=v_add(p.v,v_dif)
 end
 
 p.v=v_mul(p.v,0.8)
 
 if abs(p.v[1]) < 0.01 then
  p.v[1]=0
 end
 if abs(p.v[2]) < 0.01 then
  p.v[2]=0
 end
 
 p.p=v_add(p.p,p.v)
 
 -- camera
 cam.p=v_lerp(cam.p,v_sub(p.p,{64,64}),0.1)

 update_trees()
 
 if v_dist(p.p,footprints[#footprints]) > 5 then
  local f={p.p[1],p.p[2]}
  add(footprints,f)
 end
end


function update_trees()
 for t in all(trees.a) do
  t.s=v_sub(t.p,v_add(cam.p,{64,64}))
  t.s[1]/=trees.gap
  t.s[2]/=trees.gap
  t.s=v_mul(t.s,t.height)
  --local r=abs(s[1])+abs(s[2])/(trees.gap*)
  
  t.s=v_add(t.p,t.s)
  
  t.leaves[1]=v_lerp(t.p,t.s,0.5)
  t.leaves[2]=v_lerp(t.p,t.s,0.75)
  t.leaves[3]=t.s
 end
end


function _draw()
 camera(0,0)
 color(15)
 rectfill(0,0,127,127)
 
 camera(cam.p[1],cam.p[2])
 circ(p.p[1],p.p[2],4,0)
 line(p.p[1],p.p[2],
 p.p[1]+4*cos(p.a),
 p.p[2]+4*sin(p.a))
 
 color(5)
 for f in all(footprints) do
  circfill(f[1],f[2],1)
 end
 
 draw_trees()
 
 draw_debug()
end

function draw_trees()
 -- shadows
 color(5)
 for t in all(trees.a) do
  circfill(t.p[1],t.p[2],t.girth)
 end
 -- trunks
 color(4)
 for t in all(trees.a) do
  line(
  t.p[1],
  t.p[2],
  t.s[1],
  t.s[2])
  line(
  t.p[1]+1,
  t.p[2],
  t.s[1],
  t.s[2])
  line(
  t.p[1]-1,
  t.p[2],
  t.s[1],
  t.s[2])
  line(
  t.p[1],
  t.p[2]+1,
  t.s[1],
  t.s[2])
  line(
  t.p[1],
  t.p[2]-1,
  t.s[1],
  t.s[2])
  --circfill(t.p[1],t.p[2],1)
 end
 -- leaves
 color(3)
 for t in all(trees.a) do
  circfill(t.leaves[1][1],t.leaves[1][2],t.girth)
 end
 color(11)
 for t in all(trees.a) do
  circfill(t.leaves[2][1],t.leaves[2][2],t.girth*0.75)
 end
 color(7)
 for t in all(trees.a) do
  circfill(t.leaves[3][1],t.leaves[3][2],t.girth*0.5)  
 end
end


function draw_debug()
 camera(0,0)
 
 
 print_ol("mem:"..stat(0)/1024,1,1,0,7)
 print_ol("cpu:"..stat(1),1,7,0,7)
end

function print_ol(_s,_x,_y,_c1,_c2)
 color(_c1)
 for x=_x-1,_x+1 do
 for y=_y-1,_y+1 do
  print(_s,x,y)
 end
 end
 print(_s,_x,_y,_c2)
end
